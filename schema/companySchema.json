{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "handle",
      "name"
    ],
    "properties": {
      "handle": {
        "$id": "#/properties/handle",
        "type": "string",
        "title": "The Handle Schema",
        "default": "",
        "examples": [
          "NVDA"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "NVidia"
        ],
        "pattern": "^(.*)$"
      },
      "num_employees": {
        "$id": "#/properties/numEmployees",
        "type": "integer",
        "title": "The Numemployees Schema",
        "default": 0,
        "examples": [
          5000
        ]
      },
      "description": {
        "$id": "#/properties/description",
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "videocards"
        ],
        "pattern": "^(.*)$"
      },
      "logo_url": {
        "$id": "#/properties/logoURL",
        "type": "string",
        "title": "The Logourl Schema",
        "default": "",
        "examples": [
          "www.nvda.com"
        ],
        "pattern": "^(.*)$"
      }
    }
  }